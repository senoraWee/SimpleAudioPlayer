<Window x:Class="Simple_Audio_Player.Wadd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Simple_Audio_Player"
        mc:Ignorable="d"
        Title="Wadd" Height="300" Width="300">
    <Grid>
        <Label Content="Add this track to playlist?" HorizontalAlignment="Left" Margin="64,60,0,0" VerticalAlignment="Top" Width="178" FontSize="14"/>
        <Button x:Name="btDoNotAdd" Content="No" HorizontalAlignment="Left" Margin="31,193,0,0" VerticalAlignment="Top" Width="75" Click="btDoNotAdd_Click"/>
        <Button x:Name="btAdd" Content="Yes" HorizontalAlignment="Left" Margin="188,193,0,0" VerticalAlignment="Top" Width="75" Click="btAdd_Click"/>

    </Grid>
</Window>
